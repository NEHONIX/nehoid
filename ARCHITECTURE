# NehoID Project Structure

nehoid/
├── src/
│   ├── index.ts                 # Main export file
│   ├── core/
│   │   ├── generator.ts         # Core ID generation logic
│   │   ├── encoder.ts           # Encoding utilities
│   │   └── validator.ts         # Validation logic
│   ├── features/
│   │   ├── collision.ts         # Collision detection
│   │   ├── context.ts           # Context-aware generation
│   │   ├── migration.ts         # ID migration utilities
│   │   └── monitoring.ts        # Performance monitoring
│   ├── integrations/
│   │   ├── express.ts           # Express.js middleware
│   │   ├── mongoose.ts          # Mongoose integration
│   │   └── database.ts          # Database helpers
│   ├── utils/
│   │   ├── compression.ts       # Compression utilities
│   │   ├── crypto.ts            # Cryptographic utilities
│   │   └── helpers.ts           # General helpers
│   └── types/
│       └── index.ts             # TypeScript definitions
├── __tests__/
│   ├── core/
│   │   ├── generator.test.ts
│   │   ├── encoder.test.ts
│   │   └── validator.test.ts
│   ├── features/
│   │   ├── collision.test.ts
│   │   ├── context.test.ts
│   │   └── monitoring.test.ts
│   ├── integrations/
│   │   └── express.test.ts
│   └── utils/
│       └── helpers.test.ts
├── benchmarks/
│   ├── performance.js
│   ├── memory.js
│   └── comparison.js
├── examples/
│   ├── basic-usage.js
│   ├── express-integration.js
│   ├── collision-detection.js
│   └── batch-generation.js
├── docs/
│   ├── api.md
│   ├── migration-guide.md
│   ├── performance.md
│   └── integrations.md
├── dist/                        # Built files (generated)
├── coverage/                    # Test coverage (generated)
├── package.json
├── tsconfig.json
├── rollup.config.js
├── jest.config.js
├── .eslintrc.js
├── .gitignore
├── .npmignore
├── README.md
├── LICENSE
├── CHANGELOG.md
└── CONTRIBUTING.md